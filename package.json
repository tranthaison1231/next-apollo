{
  "name": "tpbank-white-label-web",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"src/**/*.{tsx,ts}\" --fix",
    "format": "prettier --write src",
    "pre-commit": "lint-staged",
    "check-types": "tsc --noEmit",
    "check-types:watch": "tsc --noEmit --watch",
    "lint-staged": "lint-staged",
    "lint-pass": "echo '\\033[4;32m♡ ' Format complete ♡' \\033[0m'",
    "gen:i18:keys": "ts-node --project scripts/tsconfig.json scripts/generateTranslationTypes.ts",
    "gen-graphql-types": "graphql-codegen -r dotenv/config --config codegen.yml",
    "gen-public-graphql-types": "graphql-codegen -r dotenv/config --config codegen-public.yml",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@apollo/client": "^3.6.9",
    "@emoji-mart/data": "^1.0.5",
    "@emoji-mart/react": "^1.0.1",
    "antd": "^4.22.6",
    "antd-img-crop": "^4.2.4",
    "apollo-link-error": "^1.1.13",
    "aws-amplify": "^4.3.31",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "emoji-mart": "^5.2.1",
    "firebase": "^9.9.2",
    "graphql": "^16.6.0",
    "lodash-es": "^4.17.21",
    "next": "^12.2.5",
    "next-i18next": "^11.3.0",
    "next-plugin-antd-less": "^1.8.0",
    "react": "18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "18.2.0",
    "react-google-recaptcha": "^2.1.0",
    "react-otp-input": "^2.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "11.10.0",
    "@enouvo/eslint-config-react": "^1.0.3",
    "@graphql-codegen/cli": "^2.11.6",
    "@graphql-codegen/fragment-matcher": "^3.3.1",
    "@graphql-codegen/import-types-preset": "^2.2.3",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.3",
    "@next/bundle-analyzer": "^12.2.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/emoji-mart": "^3.0.9",
    "@types/lodash-es": "^4.17.6",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.uniqby": "^4.7.7",
    "@types/node": "^18.7.6",
    "@types/prettier": "^2.7.0",
    "@types/react": "^18.0.17",
    "@types/react-google-recaptcha": "^2.1.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "autoprefixer": "^10.4.8",
    "babel-plugin-import": "^1.13.5",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "eslint": "^8.22.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.8.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "graphql-codegen-apollo-next-ssr": "^1.7.4",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "next-compose-plugins": "^2.2.1",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "yarn lint",
      "prettier --write"
    ]
  },
  "eslintIgnore": [
    "vite.config.ts",
    "scripts/*",
    "generated"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
